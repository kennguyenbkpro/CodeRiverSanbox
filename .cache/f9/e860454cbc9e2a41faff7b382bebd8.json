nerated":{"line":100,"column":15},"source":"src/menuScene.js","original":{"line":81,"column":15},"name":"MenuScene"},{"generated":{"line":100,"column":24},"source":"src/menuScene.js","original":{"line":81,"column":24}},{"generated":{"line":101,"column":0},"source":"src/menuScene.js","original":{"line":81,"column":24},"name":"exports"},{"generated":{"line":101,"column":7},"source":"src/menuScene.js","original":{"line":81,"column":24}},{"generated":{"line":101,"column":8},"source":"src/menuScene.js","original":{"line":81,"column":24},"name":"default"},{"generated":{"line":101,"column":15},"source":"src/menuScene.js","original":{"line":81,"column":24}},{"generated":{"line":101,"column":18},"source":"src/menuScene.js","original":{"line":81,"column":24},"name":"_default"},{"generated":{"line":101,"column":26},"source":"src/menuScene.js","original":{"line":81,"column":24}}],"sources":{"src/menuScene.js":"// Import the Phaser library\r\nconst Phaser = require(\"phaser\");\r\n\r\n// Create a MenuScene class that extends Phaser.Scene\r\nclass MenuScene extends Phaser.Scene {\r\n  constructor() {\r\n    super(\"MenuScene\");\r\n  }\r\n\r\n  preload() {\r\n    // Preload the background image\r\n    this.load.image(\r\n      \"background\",\r\n      \"https://vtv1.mediacdn.vn/thumb_w/640/2021/9/1/3-16304823045561332638143.png\"\r\n    );\r\n  }\r\n\r\n  create() {\r\n    // Add a background image\r\n    const backgroundImage = this.add.image(0, 0, \"background\").setOrigin(0);\r\n\r\n    // Set the scale of the background image to cover the entire game canvas\r\n    backgroundImage.setScale(\r\n      this.cameras.main.width / backgroundImage.width,\r\n      this.cameras.main.height / backgroundImage.height\r\n    );\r\n\r\n    // Add a play button with enhanced styling\r\n    const playButton = this.add\r\n      .text(\r\n        this.cameras.main.width / 2,\r\n        this.cameras.main.height / 2,\r\n        \"Chơi nào\", // Updated text: \"Chơi nào\"\r\n        {\r\n          fontFamily: \"Arial\",\r\n          fontSize: \"32px\",\r\n          color: \"#ffffff\",\r\n          backgroundColor: \"#ff6f00\", // Set a background color\r\n          padding: {\r\n            x: 20,\r\n            y: 10,\r\n          },\r\n          borderRadius: 10, // Add border radius for a rounded button appearance\r\n          stroke: \"#000000\", // Add stroke color\r\n          strokeThickness: 2, // Add stroke thickness\r\n          shadow: {\r\n            offsetX: 2,\r\n            offsetY: 2,\r\n            color: \"#000000\",\r\n            blur: 4,\r\n            fill: true,\r\n          },\r\n        }\r\n      )\r\n      .setOrigin(0.5);\r\n\r\n    // Make the play button interactive\r\n    playButton.setInteractive();\r\n\r\n    // Add hover and click effects\r\n    playButton.on(\"pointerover\", () => {\r\n      playButton.setAlpha(0.8); // Reduce opacity on hover\r\n    });\r\n\r\n    playButton.on(\"pointerout\", () => {\r\n      playButton.setAlpha(1); // Restore opacity on pointerout\r\n    });\r\n\r\n    playButton.on(\"pointerdown\", () => {\r\n      playButton.setScale(0.95); // Scale down on click\r\n    });\r\n\r\n    playButton.on(\"pointerup\", () => {\r\n      playButton.setScale(1); // Restore scale on pointerup\r\n      // Start the Reflection mini-game scene\r\n      this.scene.start(\"ReflectionScene\");\r\n    });\r\n  }\r\n}\r\n\r\nexport default MenuScene;\r\n"},"lineCount":null}},"error":null,"hash":"855caf4b9655cc77267973b93a19ca60","cacheData":{"env":{}}}